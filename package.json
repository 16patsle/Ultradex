{
  "name": "ultradex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "bulbapedia": "vite-node bulbapedia-data/bulbapedia2json.ts",
    "postinstall": "patch-package && npm run bulbapedia",
    "preinstall": "curl --data \"key=$FONTAWESOME_KEY\" https://cdn.multitek.no/packages/fontawesome-solid.php --output fontawesome-solid.tar && tar -xf fontawesome-solid.tar"
  },
  "dependencies": {
    "@16patsle/pokeapi.js": "^2.0.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/pro-solid-svg-icons": "file:fortawesome-pro-solid-svg-icons-5.15.4.tgz",
    "@fortawesome/vue-fontawesome": "^3.0.0",
    "@oruga-ui/oruga-next": "^0.5.5",
    "@oruga-ui/theme-bulma": "^0.2.6",
    "core-js": "^3.23.2",
    "patch-package": "^6.4.7",
    "pinia": "^2.0.14",
    "register-service-worker": "^1.7.2",
    "vite-node": "^0.16.0",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vue-virtual-scroller": "^2.0.0-alpha.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.36",
    "@types/xml2json": "^0.11.4",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.1.0",
    "eslint": "^8.18.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.1.1",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.2",
    "vue-tsc": "^0.38.1",
    "wtf_wikipedia": "^10.0.1",
    "xml2json": "^0.12.0",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "license": "Apache-2.0",
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ],
    "*.vue": [
      "npm run lint"
    ]
  }
}
