{
  "name": "ultradex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "bulbapedia": "vite-node bulbapedia-data/bulbapedia2json.ts",
    "postinstall": "patch-package && nuxt prepare && npm run bulbapedia",
    "preinstall": "curl --data \"key=$FONTAWESOME_KEY\" https://cdn.multitek.no/packages/fontawesome-solid.php --output fontawesome-solid.tar && tar -xf fontawesome-solid.tar"
  },
  "dependencies": {
    "@16patsle/pokeapi.js": "^2.0.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/pro-solid-svg-icons": "file:fortawesome-pro-solid-svg-icons-5.15.4.tgz",
    "@fortawesome/vue-fontawesome": "^3.0.0",
    "@oruga-ui/oruga-next": "^0.5.10",
    "@oruga-ui/theme-bulma": "^0.2.8",
    "@pinia/nuxt": "^0.4.6",
    "core-js": "^3.27.1",
    "patch-package": "^6.5.1",
    "pinia": "^2.0.28",
    "register-service-worker": "^1.7.2",
    "vite-node": "^0.27.1",
    "vue-virtual-scroller": "^2.0.0-alpha.1"
  },
  "devDependencies": {
    "@nuxt/image-edge": "1.0.0-27840416.dc1ed65",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@types/xml2json": "^0.11.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/ui": "^0.27.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.7",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.32.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "fs-extra": "^11.1.0",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.0.0",
    "prettier": "^2.8.3",
    "start-server-and-test": "^1.15.2",
    "typescript": "~4.9.4",
    "vitest": "^0.27.1",
    "vue-tsc": "^1.0.24",
    "wtf_wikipedia": "^10.0.1",
    "xml2json": "^0.12.0",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "license": "Apache-2.0",
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ],
    "*.vue": [
      "npm run lint"
    ]
  }
}
